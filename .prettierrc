{
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "printWidth": 120,
  "proseWrap": "always",
  "jsxBracketSameLine": true,
  "jsxSingleQuote": true,
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  }
}
