{
	"name": "linkedin-coya",
	"version": "0.0.1",
	"description": "Web interface for Linkedin API",
	"main": "src/server.js",
	"scripts": {
		"test": "NODE_ENV=test mocha src/tests --timeout 60000",
		"start": "NODE_ENV=development node src/server.js",
		"debug": "NODE_ENV=development node src/debug.js",
		"server": "NODE_ENV=production forever --id linkedin start src/server.js",
		"build": "webpack --mode production",
		"dev-build": "webpack --watch=true --mode development",
		"manual-login": "node scripts/manual_login.js",
		"oauth": "NODE_ENV=oauth node src/oauth.js"
	},
	"author": "Coya",
	"license": "ISC",
	"devDependencies": {
		"babel-loader": "^7.1.4",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"chai": "^4.1.2",
		"css-loader": "^0.28.11",
		"mocha": "^5.2.0",
		"node-sass": "^4.9.0",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"supertest": "^3.1.0",
		"webpack": "^4.6.0",
		"webpack-cli": "^3.1.2"
	},
	"dependencies": {
		"@coya/counter": "0.0.4",
		"@coya/logger": "0.0.2",
		"@coya/puppy": "^0.1.1",
		"@coya/utils": "^1.1.0",
		"bulma": "^0.7.1",
		"csv": "^3.0.2",
		"express": "^4.16.3",
		"mongoose": "^5.1.2",
		"node-linkedin": "^0.5.6",
		"raven": "^2.6.2",
		"request-promise": "^4.2.2",
		"sleep": "^5.1.1",
		"whatwg-fetch": "^2.0.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Cooya/Linkedin.git"
	}
}
